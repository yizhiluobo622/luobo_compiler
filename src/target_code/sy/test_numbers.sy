int main() {
    // 十进制整数
    int a = 123;
    int b = 0;
    
    // 八进制整数
    int c = 0123;  // 83 in decimal
    
    // 十六进制整数
    int d = 0x123; // 291 in decimal
    int e = 0XABC; // 2748 in decimal
    
    // 浮点数
    float f = 123.456;
    float g = 123.;
    float h = .456;
    float i = 123e+10;
    float j = 123E-10;
    
    return 0;
}
